import java.util.*;
public class SumsPairArray
{
    public static void main(String[]args)
	{
	   Scanner xyz= new Scanner(System.in);
	   
	   int target;
	   System.out.println("Enter the target element");
	   target=xyz.nextInt();
	   
	   int n;
	   System.out.println("Enter the elemnts for a array length");
	   n=xyz.nextInt();
	   
	   
	   int a[]= new int[5];
	   System.out.println("Enter the elements in the array");
	   
	   for(int i=0; i<a.length; i++)
	   {
	      a[i]=xyz.nextInt();
	   }
	   
	   System.out.println("Display the Array elements");
	   
	   for(int i=0; i<a.length; i++)
	   {
	      System.out.println(" "+a[i]);
	   }
	   System.out.println(" Now print the pair");
	   int count=0;
	   for(int i=0; i<a.length; i++)
	   {   
		   for(int j=i+1; j<a.length; j++)
		   {
			  if(a[i]+a[j]==target)
			  {   
				  System.out.println("["+a[i]+","+a[j]+"]");
				  count++;
			  }
		   }
	   }
	   System.out.println();
	   System.out.println(" The final count of sumPir is :> "+count);
	}

}